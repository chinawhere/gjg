<div class="row">
  <div class="col-md-12">
    <!-- BEGIN EXAMPLE TABLE PORTLET-->
    <div class="portlet box grey-cascade">
      <div class="portlet-title">
        <div class="caption">
          <i class="fa fa-globe"></i>用户信息
        </div>
        <div class="tools">
          <a class="collapse" href="javascript:;">
          </a>
        </div>
      </div>
      <div class="portlet-body">
        <div class="dataTables_wrapper">
          <div class="btn-group">
<!--             <a href="<%= new_admin_enlist_path %>" class="btn green">
            添加学生 <i class="fa fa-plus"></i>
            </a> -->
          </div>
        </div>
        <table class="table table-striped table-bordered table-hover dataTable">
          <thead>
            <tr>
              <th>Global Id</th>
              <th>用户名</th>
              <th>手机号</th>
              <th>邮箱</th>
              <th>性别</th>
              <th>创建时间</th>
              <!-- <th>操作</th> -->
            </tr>
          </thead>
          <% @enlists.each do |one|%>
            <tbody>
              <tr class="odd gradeX">
                <td class="ng-binding"><%= one.global_id %></td>
                <td class="ng-binding"><%= one.username %></td>
                <td class="ng-binding"><%= one.mobile %></td>
                <td class="ng-binding"><%= one.email %></td>
                <td class="ng-binding"><%= one.sex %></td>

                <td class="ng-binding"><%= time_format(one.created_at)%></td>
<!--                 <td>
                  <%= link_to '编辑', edit_admin_player_path(one) %>
                  <%= link_to '删除', admin_player_path(one), method: :delete, confirm: '确认要删除吗？' %>
                </td> -->
              </tr>
            </tbody>
          <% end -%>
        </table>
      </div>
    </div>
    <!-- END EXAMPLE TABLE PORTLET-->
  </div>
</div>