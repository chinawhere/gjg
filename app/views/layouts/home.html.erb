<!DOCTYPE html>
<html>
<head>
  <title>ChinaWhere</title>
  <%= stylesheet_link_tag    "application" %>
  <%= stylesheet_link_tag    "ui/scaffolds", "ui/ui_calendar" %>
  <%= javascript_include_tag "application" %>
  <%= javascript_include_tag "ui/ui_header" %>
  <%= javascript_include_tag "ui/ui_calendar" %>
  <%= yield :header %>
  <%= csrf_meta_tags %>
</head>
<body>

    <%= render '/layouts/header' %>
    <div class="container" style="margin-top:90px">
      <div class="row">
        <div class="col-md-3">
          <%= render '/layouts/home_sidder'%>
        </div>
        <div class="col-md-9"><%= yield %></div>
      </div>
    </div> <!-- /container -->

    <div id="footer">
      <div class="container">
        <p class="text-muted">Place sticky footer content here.</p>
      </div>
    </div>
</body>

</html>
