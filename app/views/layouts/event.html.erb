<!DOCTYPE html>
<html>
<head>
  <title>ChinaWhere</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag    'bootstrap.min'%>
  <%= stylesheet_link_tag    'bootstrap_extra' %>
  <%= stylesheet_link_tag    'font-awesome'%>
  <%= stylesheet_link_tag    'ui/scaffolds', 'ui/base' %>
  <%= javascript_include_tag 'application' %>
  <%= javascript_include_tag 'ui/ui_header' %>
  <%= yield :header %>
  <%= csrf_meta_tags %>
  <script>
    var Chinawhere = new Object;
    Chinawhere.current_user_id = <%= @current_user.try(:id) || false %>;
  </script>
</head>
<body>
    <%= render '/layouts/header' %>
    <div class="container" style="margin-top:90px">
      <%= yield %>
    </div> <!-- /container -->

    <div id="footer">
      <div class="container">
        <p class="text-muted">Place sticky footer content here.</p>
      </div>
    </div>

</body>

</html>
